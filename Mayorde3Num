#include <iostream>
//ireneramirez
using namespace std;

int main()
{
    int num1,num2,num3;
    cout<<"Primer valor"<<endl;
    cin>>num1;
    cout<<"Segundo valor"<<endl;
    cin>>num2;
    cout<<"Tercer Valor"<<endl;
    cin>>num3;

    if (num1>num2 && num3)
    {
        cout<<"el mayor es:"<<num1<<endl;
    }
    else if (num2>num3 &num1)
    {
        cout<<"el mayor es:"<<num2<<endl;
    }
    else if (num3>num1 && num2)
    {
        cout<<"el mayor es: "<<num3<<endl;
    }
    return 0;

}
